{
   "manifest_version": 3,
   "name": "black",
   "description": "choose black",
   "version": "1.0",
   "background": {
      "service_worker": "background.js",
      "matches": [
         "https://tw.piliapp.com/*"
      ]
   },
   "options_page": "options.html",
   "permissions": [
      "tabs",
      "scripting",
      "activeTab",
      "storage"
   ],
   "host_permissions": [
      "http://*/*",
      "https://*/*"
   ],
   "icons": {
      "16": "images/bet16.png",
      "32": "images/bet32.png",
      "48": "images/bet48.png",
      "128": "images/bet128.png"
   },
   "content_scripts": [
      {
         "js": [
            "content.js"
         ],
         "css": [
            "content.css"
         ],
         "matches": [
            "https://*/*",
            "http://*/*"
         ]
      }
   ],
   "commands": {}
}